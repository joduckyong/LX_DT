<script>

const commonUrl = '/common';
const url = '/dataSet';

//데이터 셋 메타정보 유형 조회
const dpIngestMetaDsetType = function () {
	
	let dset_idntfc_id = '{{dset_idntfc_id}}';
	
	const data = {
   		url: '/dp/ingest/meta/dataset/type',
   		user_id: 'user00',
   		dset_idntfc_id: dset_idntfc_id,
    };
	
    $.ajax({
        type: 'POST',
        url: url+'/dpIngestMetaDsetType',
        dataType: 'json',
        contentType: 'application/json; charset=utf-8',
        data: JSON.stringify(data)
    }).done(function (data) {
    	
	  	  if (data != null) {
	  		  
    		  if(errorMsg(data)){
   	          	 return;  
       	  	  }				
	  		
  	    	if(data.contents != ""){
  	    		for(key in data.contents[0]){
  	    			if(data.contents[0][key] == 'Y'){
  	    				$("#"+key).show();
  	    			}
  	    		}
  	    	}
	  	  }
    	  
    }).fail(function (error) {
        alert(JSON.stringify(error));
    });	
}

//데이터 셋 메타정보 조회
